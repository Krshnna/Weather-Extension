{
  "manifest_version": 3,
  "name": "Weather Extension",
  "description": "Basic Extension",
  "version": "1.0",
  "permissions": ["tabs", "background", "storage", "geolocation"],
  "host_permissions": ["https://rapidapi.com/worldapi/api/open-weather13*"],
  "optional_host_permissions": ["https://www.developer.chrome.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "fan.jpg"
  }
}
